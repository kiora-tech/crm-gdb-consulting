{#- @var companyIdentity \App\Entity\CompanyIdentity -#}
:root {
  --custom-default: {{ companyIdentity.defaultColor|raw }} !important;
  --custom-primary: {{ companyIdentity.primaryColor|raw }} !important;
  --custom-secondary: {{ companyIdentity.secondaryColor|raw }} !important;
  --custom-tertiary: {{ companyIdentity.tertiaryColor|raw }} !important;

  --custom-default-l20: {{ companyIdentity.defaultColor|colorBrighter(20)|raw }} !important;
  --custom-default-l30: {{ companyIdentity.defaultColor|colorBrighter(30)|raw }} !important;

  --custom-primary-l5: {{ companyIdentity.primaryColor|colorBrighter(5)|raw }} !important;
  --custom-primary-l8: {{ companyIdentity.primaryColor|colorBrighter(8)|raw }} !important;
  --custom-primary-l10: {{ companyIdentity.primaryColor|colorBrighter(10)|raw }} !important;
  --custom-primary-l36: {{ companyIdentity.primaryColor|colorBrighter(36)|raw }} !important;
  --custom-primary-l38: {{ companyIdentity.primaryColor|colorBrighter(38)|raw }} !important;
  --custom-primary-l58: {{ companyIdentity.primaryColor|colorBrighter(58)|raw }} !important;

  --custom-secondary-l60: {{ companyIdentity.secondaryColor|colorBrighter(60)|raw }} !important;
  --custom-secondary-l68: {{ companyIdentity.secondaryColor|colorBrighter(68)|raw }} !important;
  --custom-secondary-l76: {{ companyIdentity.secondaryColor|colorBrighter(76)|raw }} !important;

  --custom-secondary-rgba-0-1: {{ companyIdentity.secondaryColor|colorRgba(0.1)|raw }} !important;
  --custom-secondary-rgba-0-15: {{ companyIdentity.secondaryColor|colorRgba(0.15)|raw }} !important;
  --custom-secondary-rgba-0-2: {{ companyIdentity.secondaryColor|colorRgba(0.2)|raw }} !important;
  --custom-secondary-rgba-0-3: {{ companyIdentity.secondaryColor|colorRgba(0.3)|raw }} !important;
  --custom-secondary-rgba-0-5: {{ companyIdentity.secondaryColor|colorRgba(0.5)|raw }} !important;
  --custom-secondary-rgba-0-6: {{ companyIdentity.secondaryColor|colorRgba(0.6)|raw }} !important;
  --custom-secondary-rgba-1-0: {{ companyIdentity.secondaryColor|colorRgba(1.0)|raw }} !important;

  --custom-tertiary-l10: {{ companyIdentity.tertiaryColor|colorBrighter(10)|raw }} !important;
  --custom-tertiary-l20: {{ companyIdentity.tertiaryColor|colorBrighter(20)|raw }} !important;
  --custom-tertiary-l30: {{ companyIdentity.tertiaryColor|colorBrighter(30)|raw }} !important;
  --custom-tertiary-l35: {{ companyIdentity.tertiaryColor|colorBrighter(35)|raw }} !important;
  --custom-tertiary-l40: {{ companyIdentity.tertiaryColor|colorBrighter(40)|raw }} !important;
  --custom-tertiary-l50: {{ companyIdentity.tertiaryColor|colorBrighter(50)|raw }} !important;
  --custom-tertiary-l70: {{ companyIdentity.tertiaryColor|colorBrighter(70)|raw }} !important;
}